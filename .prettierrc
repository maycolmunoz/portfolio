{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
        "^react$",
        "^react-dom$",
        "^@radix-ui/(.*)$",
        "^@tabler/(.*)$",
        "^@/components/(.*)$",
        "^@/lib/(.*)$",
        "^@/utils/(.*)$",
        "^@/data/(.*)$",
        "^@/assets/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}